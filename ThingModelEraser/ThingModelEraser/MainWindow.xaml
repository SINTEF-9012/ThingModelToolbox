<Window x:Class="ThingModelEraser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ThingModelEraser" Height="160" Width="300">
    <DockPanel>
		<DockPanel DockPanel.Dock="Bottom">
			<Button Width="70" Content="Delete" Background="#4a148c" Foreground="White" Click="DeleteThing" DockPanel.Dock="Right"></Button>
			<TextBox Background="#3e2723" Foreground="White" Name="ID"></TextBox>
		</DockPanel>
		<Button Content="Clear" Background="#d01716" Foreground="White" FontSize="40" Click="EraseEverything"></Button>
    </DockPanel>
</Window>
